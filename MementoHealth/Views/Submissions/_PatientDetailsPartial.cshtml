@model MementoHealth.Entities.Patient

<div class="text-secondary">
    <b>Patient: </b> @Model.FullName |
    <b>Birthday: </b> @Model.Birthday.ToShortDateString()
    @if (!string.IsNullOrWhiteSpace(Model.ExternalPatientId))
    {
        <text>|</text>
        <b>External ID: </b> @Model.ExternalPatientId
    }
</div>